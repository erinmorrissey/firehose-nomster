<% @places.each do |place| %>
  <div class="booyah-box col-xs-10 col-xs-offset-1">
    <h1><%= place.name %></h1>
    <h2><%= place.address %></h2><br />
    <p><%= place.description %></p>
  </div>
<% end %>

<%= will_paginate @places, renderer: BootstrapPagination::Rails, previous_label: "Older", next_label: "Newer" %>